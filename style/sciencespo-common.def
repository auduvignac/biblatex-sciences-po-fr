% Shared title formatting for citations and bibliography.
\providecommand*{\scpo@titleformat}[1]{%
  \ifboolexpr{ test{\ifentrytype{article}} or test{\ifentrytype{incollection}} }
    {\mkbibquote{#1}}%
    {\mkbibemph{#1}}}
